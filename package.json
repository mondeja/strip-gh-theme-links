{
  "name": "strip-gh-theme-links",
  "version": "2.0.0",
  "description": "Strip Github dark/light theme image links",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "./index.d.ts",
  "bin": "cli.js",
  "files": ["dist/cjs", "dist/esm", "index.d.ts"],
  "scripts": {
    "clean": "rimraf dist *.tgz",
    "lint:fix": "eslint '*.{t,j}s' --fix && eslint 'tests/**/*.ts' --fix",
    "lint": "eslint '*.{t,j}s' && eslint 'tests/**/*.ts'",
    "build:action": "ncc build action/index.ts --minify --v8-cache -o dist/action",
    "build:cjs": "tsc index.ts --module commonjs --outDir dist/cjs",
    "build:esm": "tsc index.ts --module es2020 --outDir dist/esm",
    "build": "run-s build:action && run-p build:cjs build:esm",
    "test": "uvu -r tsm tests",
    "prebuild": "run-s lint:fix clean",
    "prepare": "is-ci || husky install"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "Álvaro Mondéjar Rubio",
    "email": "mondejar1994@gmail.com"
  },
  "contributors": [],
  "repository": "https://github.com/mondeja/strip-gh-theme-links-action",
  "devDependencies": {
    "@actions/core": "1.6.0",
    "@types/glob": "7.2.0",
    "@types/node": "17.0.4",
    "@typescript-eslint/eslint-plugin": "5.8.0",
    "@typescript-eslint/parser": "5.8.0",
    "@vercel/ncc": "0.33.1",
    "eslint": "8.5.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "glob": "7.2.0",
    "husky": "7.0.4",
    "is-ci": "3.0.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "tsm": "2.2.1",
    "typescript": "4.5.4",
    "uvu": "0.5.2"
  }
}
