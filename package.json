{
  "name": "strip-gh-theme-links",
  "version": "4.0.0",
  "description": "Strip Github dark/light theme image links",
  "type": "module",
  "main": "index.js",
  "bin": "cli.js",
  "license": "BSD-3-Clause",
  "files": [
    "index.js",
    "transformer.js"
  ],
  "author": {
    "name": "Álvaro Mondéjar Rubio",
    "email": "mondejar1994@gmail.com"
  },
  "contributors": [],
  "repository": "https://github.com/mondeja/strip-gh-theme-links",
  "dependencies": {
    "cheerio": "1.0.0-rc.12",
    "fake-diff": "1.0.0",
    "remark": "14.0.2"
  },
  "devDependencies": {
    "@actions/core": "1.10.0",
    "@vercel/ncc": "0.34.0",
    "eslint": "8.28.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "glob": "8.0.3",
    "husky": "8.0.2",
    "is-ci": "3.0.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "sdown": "1.0.2",
    "uvu": "0.5.6"
  },
  "scripts": {
    "format": "prettier -w .",
    "lint:fix": "eslint --fix .",
    "lint": "run-s format lint:fix",
    "build:action": "ncc build action/index.js --v8-cache -o action/dist/ --target es2020",
    "build": "run-s build:action",
    "test": "uvu",
    "prepare": "run-p setup:husky setup:si-readme",
    "setup:husky": "is-ci || husky install",
    "setup:si-readme": "sdown https://raw.githubusercontent.com/mondeja/simple-icons/readme-picture-blocks/README.md si-readme.md"
  }
}
